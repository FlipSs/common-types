  "dependencies": {
    "rxjs": "^6.5.1",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.12",
    "@types/node": "^11.13.8",
    "jasmine": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "typescript": "^3.4.5"
  }
